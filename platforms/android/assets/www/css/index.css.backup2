/* HOME PAGE
---------------------*/
.home-page {
    height: 30em;
}
.home-page > img {
    margin-left: 40px;
    margin-top: 170px;
}

.race-results {
    display: none;
}


/* RACE RESULTS
---------------------------*/
.actual-race-results {
    display: none;
}
.race-result-titles {
    margin-left: -70px;
    position: relative;
    border-bottom: 1px solid #333333;
    padding-bottom: 10px;
}

.race-result-titles li {
    color: #ED1C24;
    display: inline-table;
    font-size: 16px;
    padding-left: 50px;
    text-shadow: none;
}

.race-result-titles li:first-of-type {
    padding-left: 33px;
}

table.race-answers {
    margin-left: -30px;
}
.race-result-answers td {
    display: inline-table;
    padding-left: 50px;
    display: inline-table;
    font-size: 16px;
    text-shadow: none;
    color: #fff;
}

.race-result-answers td:first-of-type {
    padding-left: 33px;
    min-width: 119px;
}

tr.race-result-answers {
    margin-left: -70px;
    position: relative;
}

.race-banner {
    background-color: #ED1C24;
    color: #FFFFFF;
    padding: 10px;
}

.country-flag-results {
    float: right;
    position: relative;
    top: -21px;
}

