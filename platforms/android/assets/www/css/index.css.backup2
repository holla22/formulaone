    list-style: none outside none;
    padding-bottom: 4px;
    padding-left: 27px;
    padding-top: 10px;
}

.nav-buttons {
    width: 100%;
    height: 90px;
    background-image: url(data:image/svg+xml;base64,); /* layer fill content */
    background-image: -moz-linear-gradient(top, #222 119.26%, #000 125.26%); /* layer fill content */
    background-image: -o-linear-gradient(top, #222 119.26%, #000 125.26%); /* layer fill content */
    background-image: -webkit-linear-gradient(top, #222 119.26%, #000 125.26%); /* layer fill content */
    background-image: linear-gradient(top, #222 119.26%, #000 125.26%); /* layer fill content */
    /*overflow-x: auto;
    overflow-y: hidden;*/
    width: 100%;
}



.rich {
