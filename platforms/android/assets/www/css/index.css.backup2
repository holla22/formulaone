/* FONTS 
---------------------------------------*/
@font-face {
    font-family: 'bankgothicbold';
    src: url('../fonts/bankgothic_bold-webfont.eot');
    src: url('../fonts/bankgothic_bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/bankgothic_bold-webfont.woff') format('woff'),
         url('../fonts/bankgothic_bold-webfont.ttf') format('truetype'),
         url('../fonts/bankgothic_bold-webfont.svg#bankgothicbold') format('svg');
    font-weight: normal;
    font-style: normal;

}


/* HEADER NAV
---------------------------------------*/
.top-menu-list li {
    list-style: none outside none;
    float: left;
}
.home-button {
    float: left;
}
.f1-logo {
    background: url("../img/skewed_print/skewed_print.png") repeat scroll 0 0 rgba(0, 0, 0, 0);
    color: #FA121B;
    float: left;
    font-size: 46px;
    font-style: italic;
    height: 54px;
    width: 87.8%;
}

.logo-text {
    font-size: 34px;
    margin-left: 100px;
    margin-top: 8px;
    width: 33%;
}
.logo-text span {
    color: #fff;
}
.logo-text > img {
    margin-top: -8px;
    width: 140px;
}

/* BODY 
----------------------------------------*/
html, body {
    /*background: url("../img/drawable-xhdpi/binding_dark_@2X.png") repeat;*/
}
.body-app{
    /*background-attachment: scroll, scroll;
    background-clip: border-box, border-box;
    background-color: rgba(0, 0, 0, 0);
    background-image: url("../img/drawable-xhdpi/background.png"), url("../img/drawable-xhdpi/background_2.png");
    background-origin: padding-box, padding-box;
    background-position: 0 0%, 0 0;
    background-repeat: no-repeat, repeat;
    background-size: calc();*/
    
    background: url("../img/drawable-xhdpi/binding_dark_@2X.png") repeat;
}
.background-body {
    height: 100%;
}

/* TEAMS DIV 
---------------------------------------*/
h2 {
    color: #CD0E25;
    text-shadow: none;
    font-family: "bankgothicbold";
    text-transform: uppercase;
    
}

h4 {
    color: #FFFFFF;
    margin-bottom: -2px;
    text-shadow: none;
    font-family: Verdana,Geneva,sans-serif;
    text-transform: uppercase;
}

p div {
    color: #FFFFFF;
    font-family: Verdana,Geneva,sans-serif;
    font-size: 12px;
    position: relative;
    text-shadow: none;
    top: 12px;
}

hr {
    color: red;
}

.row .row {
    padding: 0 20px;
}

.racer-team-img ul li {
    font-family: Verdana,Geneva,sans-serif;
    font-size: 12px;
    list-style: none outside none;
    text-shadow: none;
}

.racer-team-img > ul {
    margin-left: 60px;
}

.racer-team-img  span.titles {
    color: #CD0E25;
}
.racer-team-img  span.answer {
    color: #FFFFFF;
    margin-left: 4px;
}

.racer-team {
    border-bottom: 1px solid #CD0E25;
    margin-bottom: 2px;
    padding-bottom: 10px;
}

.racer-team-img > img {
    float: left;
}

.race-car {
    width: 100%;
}


/*.race-results { display: none; }*/


/* NEWS
----------------------------------------*/

.f1-news {
    display:none;
    margin-top: 20px;
}
.img-news {
    float: left;
    width: 150px;
}

.small-12.background-body.f1-news > h4 {
    clear: right;
    font-family: "bankgothicbold";
    font-size: 21px;
    margin: 0 9px 5px 150px;
    padding-left: 10px;
    width: auto;
}

.news-date {
    color: #C90E25;
    font-size: 10px;
}

.excerpt {
    color: #FFFFFF;
    
    font-family: Verdana,Geneva,sans-serif;
    font-size: 16px;
    left: 9px;
    line-height: 23px;
    margin-bottom: 30px;
    padding-left: 0;
    padding-right: 20px;
    position: relative;
    text-shadow: none;
    top: 12px;
    width: auto;
    margin-left: 150px;
}

.row.news {
    border-bottom: 1px solid #393d44;
    margin-bottom: 11px;
}

.small-12.background-body.f1-news {
    margin-left: 11px;

}


.background-body-opacity {
    background-color: #000000;
    height: 10.5em;
    margin-bottom: 98px;
    opacity: 0.3;
    width: 100%;
}

/* HALL OF FAME
---------------------------------------*/
.hall-of-fame li {
    color: #FFFFFF;
    font-size: 16px;
    line-height: 24px;
    list-style: none outside none;
    text-shadow: none;
    font-family: Verdana,Geneva,sans-serif;
}

.hall-of-fame li span {
    color: #CD0E1E;
}

.hall-of-fame .small-12 > ul {
    margin-left: 30px;
    margin-top: -32px;
}

.f1-halloffame {
    display: none;
}

/* SINGEL CONTENT 
---------------------------------------*/
.full-content-view .small-12 {
