}

.more-data {
    display: none;
    text-shadow: none;
    color: #fff;
    line-height: 23px;
}

/* FOOTER AND BUTTONS 
---------------------------------------*/
ul.buttons-list {
    left: -35px;
    position: relative;
    top: -12px;
    width: 49em;
    height: 50px;
}

.buttons-list li {
   /* float: left;*/
